{
  "name": "@apollosproject/data-connector-rock-content",
  "version": "0.6.0",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/data-schema": "file:../apollos-data-schema",
    "@apollosproject/server-core": "file:../apollos-server-core"
  },
  "peerDependencies": {
    "@apollosproject/config": "file:../apollos-config",
    "@apollosproject/rock-apollo-data-source": "file:../apollos-rock-apollo-data-source"
  },
  "devDependencies": {
    "babel-preset-apollos": "file:../babel-preset-apollos",
    "jest": "^23.6.0"
  }
}
