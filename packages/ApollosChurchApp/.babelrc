{
  "presets": ["react-native", "module:react-native-dotenv"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-export-default-from"],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "extensions": [".android.js", ".ios.js", ".js", ".graphql"],
        "alias": {
          "apollos-church-api": "../apollos-church-api/src",
          "apolloschurchapp": "../apolloschurchapp/src"
        }
      }
    ],
    ["import-graphql"]
  ]
}